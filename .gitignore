# 通用操作系统自动生成文件
.DS_Store             # macOS系统文件
Thumbs.db             # Windows缩略图缓存
Desktop.ini           # Windows文件夹配置

# Flutter/Dart专用
.dart_tool/           # Dart工具生成目录
.packages             # 包依赖清单
.pub-cache/           # pub缓存目录
.pub/                 # pub本地配置
.flutter-plugins      # 插件配置
.flutter-plugins-dependencies
*.swp                 # Vim临时文件
*.swo
.fvm/                 # Flutter版本管理工具目录

# 构建产物
build/                # 所有构建输出
.android/             # Android构建配置（如手动生成）
.ios/                 # iOS构建配置（如手动生成）
.web/                 # Web构建配置
/project/coverage/    # 测试覆盖率报告

# 依赖管理
pubspec.lock          # 锁定依赖版本（可选，团队协作时建议保留）
Podfile.lock          # iOS依赖锁定文件（建议保留）
Pods/                 # iOS CocoaPods依赖目录

# IDE配置
.vscode/              # VS Code设置
.idea/                # JetBrains系列IDE配置
*.iml                 # IntelliJ模块文件
*.iws                 # IntelliJ工作区设置
*.ipr                 # IntelliJ项目配置

# 本地环境变量
.env                  # 环境变量文件
.env.*                # 多环境配置（如.env.prod）

# 调试文件
*.log                 # 所有日志文件
flutter_*.log         # Flutter特定日志
debug/                # 调试输出目录
profile/              
release/